	"console.writeline":{
		"prefix":"cwl",
		"body":"Console.WriteLine($1);",
		"description": "abreviacion cwl"

	},
	"console.Readline":{
		"prefix":"crl",
		"body":"Console.ReadLine();",
		"description": "abreviacion crl"

	},
	"console.readkey":{
		"prefix":"crk",
		"body":"Console.ReadKey();",
		"description": "abreviacion crk"
	},
